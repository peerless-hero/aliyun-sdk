{
  "name": "@peerless-hero/aliyun-sdk",
  "version": "0.2.0",
  "packageManager": "pnpm@9.15.1",
  "description": "Aliyun sdk for Node.js",
  "author": "peerless_hero <peelrss_hero@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/peerless-hero/aliyun-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peerless-hero/aliyun-sdk.git"
  },
  "bugs": "https://github.com/peerless-hero/aliyun-sdk/issues",
  "keywords": [
    "aliyun",
    "sdk",
    "node.js"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./versions/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "README.zh-cn.md",
    "dist"
  ],
  "engines": {
    "node": ">=17.5.0"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "dev": "tsx src/index.ts",
    "import": "tsx test/import.ts",
    "test": "vitest",
    "test:client": "tsx src/client/test.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "bump:prerelease": "changelogen --bump --release --prerelease --push",
    "bump:patch": "changelogen --bump --release --patch --push",
    "bump:minor": "changelogen --bump --release --minor --push",
    "bump:major": "changelogen --bump --release --major --push"
  },
  "dependencies": {
    "consola": "^3.3.0",
    "fast-querystring": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@antfu/ni": "^0.23.2",
    "@antfu/utils": "^0.7.10",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@wener/client": "^1.0.23",
    "bumpp": "^9.9.1",
    "ejs": "^3.1.10",
    "eslint": "^9.17.0",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.11",
    "pnpm": "^9.15.1",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unbuild": "^3.0.1",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "volta": {
    "node": "22.12.0",
    "pnpm": "9.15.1"
  }
}
